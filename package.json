{
  "name": "riiiact-kit",
  "version": "0.3.1",
  "description": "riiid flavored react starter kit.",
  "main": "index.js",
  "scripts": {
    "new": "npm install --depth 0 && node scripts/new",
    "start": "webpack-dev-server --config build/webpack-dev-server.config.js --hot --inline --progress",
    "build": "rimraf dist && webpack --config build/webpack-build.config.js --progress --colors",
    "build:prod": "rimraf dist && TARGET=production webpack --config build/webpack-build.config.js --progress --colors",
    "test": "jest"
  },
  "devServer": {
    "port": 3010
  },
  "homepage": "https://riiiact-kit-dev.firebaseapp.com/",
  "author": "kykim",
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.12.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.1",
    "babel-jest": "^13.2.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "bootstrap-sass": "^3.3.6",
    "colors": "^1.1.2",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.0.1",
    "eslint-config-google": "^0.6.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-react": "^5.2.2",
    "file-loader": "^0.9.0",
    "firebase": "^3.1.0",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^1.8.0",
    "jest-cli": "^13.2.3",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.2",
    "moment": "^2.14.1",
    "node-sass": "^3.8.0",
    "prompt": "^1.0.0",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-bootstrap": "^0.29.5",
    "react-dom": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-select": "^1.0.0-beta13",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.3",
    "rx": "^4.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "modulePaths": [
      "lib"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/",
      "node_modules/react-dom/",
      "node_modules/react-bootstrap/",
      "node_modules/material-ui/",
      "node_modules/react-addons-test-utils/"
    ]
  }
}
