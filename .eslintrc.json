{
    "parser": "babel-eslint",
    "extends": ["google", "plugin:react/recommended"],
    "plugins": ["react"],
    "globals": {
      "document": true,
      "window": true,
      "__TARGET__": true,
      "__FIREBASE_URL__": true,
      "__FIREBASE_AUTH__": true,
      "__FIREBASE_APIKEY__": true,
      "__VERSION__": true,
      "__DEPENDENCIES__": true,
      "__TITLE__": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [2, "as-needed"],
      "comma-dangle": [2, "never"],
      "camelcase": 0,
      "require-jsdoc": 0,
      "react/display-name": [2, {"ignoreTranspilerName": true}]
    }
}
